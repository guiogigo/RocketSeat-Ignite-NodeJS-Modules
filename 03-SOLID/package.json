    "@types/supertest": "6.0.2",
    "npm-run-all": "4.1.5",
    "prisma": "5.14.0",
    "supertest": "7.0.0",
    "tsup": "8.0.2",
    "tsx": "4.10.2",
    "typescript": "5.4.5",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.0.4"
  },
  "dependencies": {
    "@fastify/cookie": "9.3.1",
    "@fastify/jwt": "8.0.1",
